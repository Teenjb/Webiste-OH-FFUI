{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "preorder": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::preorder.preorder",
      "inversedBy": "user"
    },
    "fullName": {
      "type": "string",
      "required": false
    },
    "schoolOrigin": {
      "type": "string",
      "required": false
    },
    "address": {
      "type": "text",
      "required": false
    },
    "phoneNumber": {
      "type": "string",
      "unique": true
    },
    "competitions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::competition.competition",
      "mappedBy": "users_permissions_user"
    },
    "ticket": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ticket.ticket",
      "inversedBy": "user"
    }
  }
}
